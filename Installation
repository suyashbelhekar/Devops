Terminal session 1 : Creating a "Hello World" Java Application  

java -version
sudo apt update 
sudo apt install openjdk-11-jdk 

mkdir hello-world-java
cd hello-world-java
vim HelloWorld.java
javac HelloWorld

git --version
git apt update
sudo apt install git -y

git init
git add .
git status 
git config --global user.email "example.com"
git commit -m "initialized Hello World"
 
git remote add origin https://github.com/your-username/my-repo.git
git remote -v
git branch -M main
git pull --rebase origin main
git push -u origin main

pipeline { 
agent any 
stages { 
stage('Clone') { 
steps { 
git branch: 'main', url: 'https://github.com/your-username/hello-world-java.git ' 
} 
} 
stage('Build') { 
steps { 
sh 'javac HelloWorld.java' 
} 
} 
stage('Run') { 
steps { 
sh 'java HelloWorld' 
} 
} 
} 
} 
post { 
success { 
echo 'Build and execution successful!' 
} 
failure { 
echo 'Build or execution failed!' 
} 
}
sudo apt remove jenkins
sudo systmctl status jankins
